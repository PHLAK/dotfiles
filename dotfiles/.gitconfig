[user]
    name = Chris Kankiewicz
    email = Chris@ChrisKankiewicz.com
[branch "master"]
    remote = origin
    merge = refs/heads/master
[color]
    ui = auto
[core]
    autocrlf = input
    editor = nano
    excludesfile = ~/.gitignore_global
[push]
    default = simple
[remote "origin"]
    push = HEAD
[alias]
    unstage = reset HEAD --
	lg = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset'
